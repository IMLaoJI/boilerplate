{
  "name": "@effect-ts-app/boilerplate-e2e",
  "version": "1.0.0",
  "license": "NONE",
  "private": true,
  "scripts": {
    "dev": "cypress open",
    "test": "cd ../packages/prelude && yarn build2:cjs && cd ../types && yarn build2:cjs && cd ../client && yarn build2:cjs && cd ../../e2e && npx playwright test",
    "test:watch": "npx playwright-watch test",
    "lint": "NODE_OPTIONS=--max-old-space-size=6144 ESLINT_TS=1 eslint .",
    "lint:watch": "ESLINT_TS=1 esw -w --changed --clear --ext ts,tsx .",
    "prettier": "prettier \"**/*.ts\"",
    "autofix": "yarn lint --fix",
    "up": "yarn update && yarn exec-update",
    "update": "yarn ncu -u",
    "exec-update": "yarn && yarn upgrade",
    "ncu": "ncu -x cypress",
    "watch": "tsc --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.18.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/node": "^7.18.9",
    "@cypress/webpack-preprocessor": "^5.12.0",
    "@playwright/test": "~1.24.0",
    "@types/node": "^18.0.6",
    "babel-loader": "^8.2.5",
    "cypress": "^7.7.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-plugin-tab": "^1.0.5",
    "date-fns": "^2.29.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "npm-check-updates": "^15.3.4",
    "prettier": "^2.7.1",
    "puppeteer": "^15.5.0",
    "ts-loader": "^9.3.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0"
  },
  "dependencies": {
    "@effect-ts-app/boilerplate-client": "*",
    "@effect-ts-app/boilerplate-types": "*",
    "tsconfig-paths": "^4.0.0"
  }
}