{
  "name": "@effect-ts-app/react",
  "version": "1.3.0",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@effect-ts/core": "*",
    "@effect/core": "*",
    "@tsplus/stdlib": "*",
    "cross-fetch": "^3.1.5",
    "path-parser": "^6.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.25",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "react": "^18.2.0"
  },
  "scripts": {
    "build": "pnpm clean && pnpm ets:tsc --build && pnpm build2:mjs && pnpm build2:cjs && pnpm generate-exports",
    "build2:cjs": "pnpm ets:babel _esm --config-file ../../.babel.cjs.json --out-dir _cjs --out-file-extension .js --source-maps",
    "build2:mjs": "pnpm ets:babel _esm --config-file ../../.babel.mjs.json --out-dir _mjs --out-file-extension .mjs --source-maps",
    "clean": "rm -rf ./_esm && rm -rf ./_mjs && rm -rf ./_cjs && find . | egrep '\\.m?js(\\.map)?$|\\.ts(\\.map)?$|\\.tsbuildinfo' | grep -v /_src/ | grep -v .eslint | grep -v jest.config | xargs rm -f",
    "postpublish": "pnpm clean",
    "generate-exports": "sh ../../utils/extract.sh > package.log",
    "circular": "madge --circular --ts-config ./tsconfig.json --extensions ts ./",
    "compile": "pnpm ets:tsc --noEmit",
    "lint": "eslint .",
    "prettier": "prettier \"**/*.ts\"",
    "autofix": "pnpm lint --fix && pnpm prettier --write",
    "test": "echo 'no tests yet'",
    "testsuite": "pnpm compile && pnpm lint && pnpm test && pnpm circular",
    "ncu": "ncu"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./_cjs/index.cjs"
      },
      "import": "./dist/index.js"
    },
    "./data": {
      "require": {
        "types": "./dist/data.d.ts",
        "default": "./_cjs/data.cjs"
      },
      "import": "./dist/data.js"
    },
    "./hooks": {
      "require": {
        "types": "./dist/hooks.d.ts",
        "default": "./_cjs/hooks.cjs"
      },
      "import": "./dist/hooks.js"
    },
    "./context": {
      "require": {
        "types": "./dist/context.d.ts",
        "default": "./_cjs/context.cjs"
      },
      "import": "./dist/context.js"
    },
    "./*.js": {
      "require": {
        "types": "./dist/*.d.ts",
        "default": "./_cjs/*.cjs"
      },
      "import": "./dist/*.js"
    },
    "./*": {
      "require": {
        "types": "./dist/*.d.ts",
        "default": "./_cjs/*.cjs"
      },
      "import": "./dist/*.js"
    }
  },
  "sideEffects": false,
  "gitHead": "ccd5a7c229c825a78737b4225f5f966d793a159c"
}