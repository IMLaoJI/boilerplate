{
  "name": "@effect-ts-app/boilerplate-client",
  "version": "0.5.0",
  "license": "MIT",
  "type": "module",
  "resolutions": {
    "typescript": "https://cdn.jsdelivr.net/npm/@tsplus/installer@0.0.127/compiler/typescript.tgz"
  },
  "peerDependencies": {
    "@effect-ts-app/boilerplate-types": "*"
  },
  "dependencies": {
    "@effect-ts-app/boilerplate-types": "*",
    "auth0": "2.42.0",
    "cross-fetch": "^3.1.5",
    "path-parser": "^6.1.0"
  },
  "devDependencies": {
    "@jest/globals": "28.1.3"
  },
  "exports": {
    "./HelloWorld": {
      "import": {
        "types": "./dist/HelloWorld.d.ts",
        "default": "./dist/HelloWorld.js"
      },
      "require": {
        "types": "./dist/HelloWorld.d.ts",
        "default": "./_cjs/HelloWorld.cjs"
      }
    },
    "./clientFor": {
      "import": {
        "types": "./dist/clientFor.d.ts",
        "default": "./dist/clientFor.js"
      },
      "require": {
        "types": "./dist/clientFor.d.ts",
        "default": "./_cjs/clientFor.cjs"
      }
    },
    "./config": {
      "import": {
        "types": "./dist/config.d.ts",
        "default": "./dist/config.js"
      },
      "require": {
        "types": "./dist/config.d.ts",
        "default": "./_cjs/config.cjs"
      }
    },
    "./errors": {
      "import": {
        "types": "./dist/errors.d.ts",
        "default": "./dist/errors.js"
      },
      "require": {
        "types": "./dist/errors.d.ts",
        "default": "./_cjs/errors.cjs"
      }
    },
    "./fetch": {
      "import": {
        "types": "./dist/fetch.d.ts",
        "default": "./dist/fetch.js"
      },
      "require": {
        "types": "./dist/fetch.d.ts",
        "default": "./_cjs/fetch.cjs"
      }
    },
    "./url": {
      "import": {
        "types": "./dist/url.d.ts",
        "default": "./dist/url.js"
      },
      "require": {
        "types": "./dist/url.d.ts",
        "default": "./_cjs/url.cjs"
      }
    },
    "./*.js": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      },
      "require": {
        "types": "./dist/*.d.ts",
        "default": "./_cjs/*.cjs"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      },
      "require": {
        "types": "./dist/*.d.ts",
        "default": "./_cjs/*.cjs"
      }
    }
  },
  "scripts": {
    "build": "yarn clean-dist && NODE_OPTIONS=--max-old-space-size=6144 ttsc --build",
    "build2:cjs": "rm -rf _cjs && babel dist --config-file ../../.babel.cjs.json --out-dir _cjs --out-file-extension .cjs --source-maps",
    "watch": "yarn build --watch",
    "watch2": "yarn clean-dist && NODE_OPTIONS=--max-old-space-size=6144 ttsc -w",
    "prepare": "yarn build",
    "clean": "rm -rf dist && rm -rf _cjs",
    "clean-dist": "sh ../../scripts/clean-dist.sh",
    "circular": "madge --circular --ts-config ./tsconfig.json --extensions ts ./_src",
    "circular:dist": "madge --circular --extensions js ./dist",
    "compile": "NODE_OPTIONS=--max-old-space-size=6144 ttsc --noEmit",
    "lint": "NODE_OPTIONS=--max-old-space-size=6144 ESLINT_TS=1 eslint .",
    "lint:watch": "ESLINT_TS=1 esw -w --changed --clear --ext ts,tsx .",
    "prettier": "prettier \"**/*.ts\"",
    "autofix": "yarn lint --fix",
    "test": "TEST_COMPILED=1 jest",
    "testsuite": "yarn lint && yarn test --passWithNoTests && yarn circular && yarn circular:dist",
    "client": "node --experimental-specifier-resolution=node -r source-map-support/register -r @effect-ts/system/Tracing/Enable ./dist/client.bin.js",
    "ncu": "ncu -x date-fns"
  },
  "gitHead": "bd8e27eea3eff97db8739d577d67e7336c078d28",
  "sideEffects": false
}