{
  "name": "@effect-ts-app/boilerplate-api",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "circular": "madge --circular --ts-config ./tsconfig.json --extensions ts ./_src",
    "circular:dist": "madge --circular --extensions js ./dist",
    "clean": "rm -rf dist",
    "clean-dist": "sh ../../scripts/clean-dist.sh",
    "build": "yarn clean-dist && NODE_OPTIONS=--max-old-space-size=6144 ttsc --build",
    "watch": "yarn build --watch",
    "watch2": "yarn clean-dist && NODE_OPTIONS=--max-old-space-size=6144 ttsc -w",
    "compile": "NODE_OPTIONS=--max-old-space-size=6144 ttsc --noEmit",
    "lint": "NODE_OPTIONS=--max-old-space-size=6144 ESLINT_TS=1 eslint _src",
    "lint:watch": "ESLINT_TS=1 esw -w --changed --clear --ext ts,tsx _src",
    "prettier": "prettier \"_src/**/*.ts\"",
    "autofix": "yarn lint --fix",
    "test": "TEST_COMPILED=1 jest",
    "testsuite": "yarn lint && yarn circular && yarn circular:dist && yarn test",
    "debug": "node --inspect --experimental-specifier-resolution=node -r source-map-support/register -r @effect-ts/system/Tracing/Enable ./dist/main.js",
    "dev": "yarn dev:compiled:run",
    "dev:traced": "NODE_ENV=development nodemon --exec yarn debug:traced",
    "dev:compiled": "yarn build && concurrently -k -p \"[{name}]\" -n \"Build,Runtime\" -c \"cyan.bold,green.bold,blue.bold\" \"yarn watch\" \"yarn dev:compiled:watch\"",
    "dev:compiled:run": "NODE_ENV=development nodemon --config nodemon.compiled.json --exec yarn start:compiled",
    "start": "yarn start:compiled",
    "start:compiled": "node --experimental-specifier-resolution=node -r source-map-support/register -r @effect-ts/system/Tracing/Enable ./dist/main.js",
    "ncu": "ncu -x applicationinsights,faker,date-fns,redis,@types/redis",
    "extract:i18n": "formatjs extract './**/*.ts' --ignore './**/*.d.ts' --format _src/i18n/extraction-formatter.cjs --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file _src/i18n/extracted/en.json",
    "e2e:circular": "cd _test && madge --circular --ts-config ./tsconfig.json --extensions ts ./_src",
    "e2e:circular:dist": "cd _test && madge --circular --extensions js ./dist",
    "e2e:clean": "sh ../../scripts/clean-dist.sh",
    "e2e:build": "yarn e2e:clean && cd _test && NODE_OPTIONS=--max-old-space-size=6144 ttsc --build",
    "e2e:watch": "yarn e2e:build --watch",
    "e2e:watch2": "yarn e2e:clean && cd _test && NODE_OPTIONS=--max-old-space-size=6144 ttsc -w",
    "e2e:compile": "cd _test && NODE_OPTIONS=--max-old-space-size=6144 ttsc --noEmit",
    "e2e:lint": "cd _test && NODE_OPTIONS=--max-old-space-size=6144 ESLINT_TS=1 eslint _src/",
    "e2e:lint:watch": "cd _test && ESLINT_TS=1 esw -w --changed --clear --ext ts,tsx _src/",
    "e2e:prettier": "prettier \"_test/_src/**/*.ts\"",
    "e2e:autofix": "yarn e2e:lint --fix",
    "e2e:test": "cd _test && TEST_COMPILED=1 jest -c ./jest.config.cjs",
    "e2e:testsuite": "yarn e2e:lint && yarn e2e:test && yarn e2e:circular && yarn e2e:circular:dist"
  },
  "exports": {
    "./*": {
      "require": "./_src/*.ts",
      "import": "./dist/*.js"
    }
  },
  "resolutions": {
    "typescript": "https://cdn.jsdelivr.net/npm/@tsplus/installer@0.0.147/compiler/typescript.tgz"
  },
  "dependencies": {
    "@effect-ts-app/boilerplate-client": "*",
    "@effect-ts-app/boilerplate-types": "*",
    "@azure/arm-monitor": "^7.0.0",
    "@azure/cosmos": "^3.17.1",
    "@azure/service-bus": "^7.7.2",
    "@azure/storage-blob": "^12.12.0",
    "@effect-ts-app/infra": "1.0.15",
    "@effect-ts/cache": "^0.3.0",
    "@effect-ts/node": "0.39.0",
    "@formatjs/cli": "^5.1.3",
    "@formatjs/intl": "^2.5.1",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/node": "^7.16.0",
    "auth0": "^2.44.0",
    "body-parser": "^1.20.1",
    "botbuilder": "^4.17.1",
    "cors": "^2.8.5",
    "date-fns": "2.23.0",
    "dotenv": "^16.0.3",
    "draft-js": "^0.11.7",
    "express": "^4.18.2",
    "express-jwt": "^7.7.5",
    "express-jwt-authz": "^2.4.1",
    "faker": "^5.5.3",
    "fast-check": "^3.3.0",
    "jest-esm-transformer": "^1.0.0",
    "jwks-rsa": "^2.1.4",
    "jwt-decode": "^3.1.2",
    "redis": "^3.1.2",
    "redlock": "^4.2.0",
    "redoc": "^2.0.0",
    "redoc-express": "^1.0.0",
    "source-map-support": "^0.5.21",
    "stopwatch-node": "^1.1.0",
    "swagger-ui-express": "^4.5.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@jest/globals": "29.2.1",
    "@types/auth0": "^2.35.6",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/draft-js": "^0.11.9",
    "@types/express": "^4.17.14",
    "@types/redis": "^2.8.32",
    "@types/swagger-ui-express": "^4.1.3",
    "eslint-plugin-formatjs": "^4.3.4"
  }
}