{
    "name": "@effect-ts-app/boilerplate-frontend-react",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "clean": "rm -rf .next pages ssr components hooks layouts i18n styles utils types tsconfig.build.tsbuildinfo tsconfig.tsbuildinfo",
        "dev": "cd ../../packages/prelude && yarn build && cd ../../apps/frontend && nodemon -x 'yarn next:dev'",
        "dev:infinite": "nodemon -x '/bin/sh -c \"yarn next:dev || touch next.config.js\"'",
        "circular": "madge --circular --ts-config ./tsconfig.json --extensions ts ./src",
        "next:dev": "NODE_OPTIONS='--max-old-space-size=6144 --inspect=127.0.0.1:9230' next dev",
        "dev:debug": "NODE_OPTIONS='--max-old-space-size=6144 --inspect=127.0.0.1:9230' next dev",
        "build:dev": "NODE_OPTIONS=--max-old-space-size=6144 ttsc --build",
        "watch": "yarn build --watch",
        "compile": "ttsc",
        "build": "rm -rf i18n && cp -r src/i18n i18n && NODE_OPTIONS=--max-old-space-size=6144 ttsc --build tsconfig.build.json",
        "build-next": "next build",
        "start": "next start -p ${PORT:-3000}",
        "lint": "NODE_OPTIONS=--max-old-space-size=6144 ESLINT_TS=1 eslint src",
        "lint:watch": "ESLINT_TS=1 esw -w --changed --clear --ext ts,tsx src",
        "prettier": "prettier \"src/**/*.ts\"",
        "autofix": "yarn lint --fix",
        "ncu": "ncu -x applicationinsights,intro.js,date-fns,@date-io/date-fns,notistack",
        "test": "TEST_COMPILED=1 jest",
        "testsuite": "yarn lint && yarn test --passWithNoTests && yarn circular",
        "update": "yarn ncu -u",
        "extract:i18n": "formatjs extract 'src/**/*.{ts,tsx}' --format src/i18n/extraction-formatter.js --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file src/i18n/extracted/en.json"
    },
    "resolutions": {
        "@types/react": "^18.0.15",
        "typescript": "https://cdn.jsdelivr.net/npm/@tsplus/installer@0.0.127/compiler/typescript.tgz"
    },
